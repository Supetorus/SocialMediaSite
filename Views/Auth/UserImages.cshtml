@model IEnumerable<string>

<div class="page-container">
	<a href="/u/@ViewBag.Username">Back to @ViewBag.Username</a>
	@if (Model.Count() == 0)
	{
		<div class="box-element">
			<p>This user has no images yet! Why don't you post one on their page.</p>
		</div>
	}
	else
	{
		<div class="box-element row-center">
			@foreach (string image in Model)
			{
				<partial name="_Image" for="@image" />
			}
		</div>
	}
</div>